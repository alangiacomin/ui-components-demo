{
  "bundleDependencies": false,
  "deprecated": false,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "lint": "eslint --ext .jsx src/**/*.{js,jsx}",
    "lint:watch": "esw --watch --color --ext .jsx src/**/*.{js,jsx}",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch",
    "typings": "copy index.js index.d.ts",
    "v:patch": "npm run createIndex && npm version patch && git push origin master --tags && npm run v:publish",
    "v:publish": "npm publish --access public",
    "util": "mkdir src\\utils\\%npm_config_name% && echo.>src\\utils\\%npm_config_name%\\%npm_config_name%.js && echo.>src\\utils\\%npm_config_name%\\%npm_config_name%.test.js",
    "createIndex": "node scripts/createIndex.js"
  },
  "sideEffects": false,
  "dependencies": {
    "@alangiacomin/ui-components": "0.0.2",
    "@reduxjs/toolkit": "^1.5.0",
    "connected-react-router": "^6.9.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "laravel-mix": "^6.0.13",
    "laravel-mix-transpile-node-modules": "^2.0.1",
    "postcss": "^8.1.14"
  }
}
